%description:
Tests the IdealClock submodule

%file: package.ned
package @TESTNAME@;
@namespace(@TESTNAME@);

%file: test.ned
package @TESTNAME@;

import nesting.ieee8021q.clock.IClock;

network Test
{
    @display("bgb=280,213");
    submodules:
        clock: <"IdealClock"> like IClock {
            @display("p=87,103");
        }
        testClock: TestClock {
            @display("p=182.70667,102.24");
        }
}

%inifile: omnetpp.ini
[General]
network = Test
sim-time-limit = 1ms
record-eventlog = true
check-signals = false
debug-on-errors = true

%exitcode: 0